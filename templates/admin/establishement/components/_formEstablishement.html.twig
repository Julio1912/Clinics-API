{{ form_start(formEstablishement)  }}
<div class="row">
    <div class="col-lg-6">
        <div class="form-group">
            {{ form_row(formEstablishement.name) }}
        </div>
    </div>
    <div class="col-lg-6">
        <div class="form-group">
            {{ form_row(formEstablishement.address) }}
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-6">
        <div class="form-group">
            {{ form_row(formEstablishement.phone) }}
        </div>
    </div>
    <div class="col-lg-6">
        <div class="form-group">
            {{ form_row(formEstablishement.status) }}
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 select-multiple">
         <div class="form-group w-100">
            {{ form_row(formEstablishement.position ) }}
        </div>
    </div>
</div>


<div class="row"> 
    {% if editMode %}
        <div class="col-lg-6">
            <button type="submit" class="btn btn-success btn-block">Modifier</button>
        </div>
    {% else %}
        <div class="col-lg-6">
            <button type="submit" class="btn btn-info btn-block">Ajouter</button>
        </div>
    {% endif %}
    
    <div class="col-lg-6">
	    <button type="reset" class="btn btn-default btn-block">Reseter</button>
    </div>
</div>

{{ form_end(formEstablishement) }}
